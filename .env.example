# Only PRIVATE_KEY is required to launch the agent; everything else falls back to defaults.

# Agent HTTP configuration
# Optional. Port that Bun will listen on locally.
PORT=8787
# Optional. Base URL surfaced in the agent manifest for absolute links.
API_BASE_URL=http://localhost:8787

# Chain configuration
# Optional. RPC endpoint used for ERC-8004 trust interactions.
RPC_URL=
# Optional. Set to true to auto-register the agent identity at startup.
REGISTER_IDENTITY=false
# Optional. Payment network identifier (e.g. base, base-sepolia).
NETWORK=base-sepolia
# Optional unless monetization is enabled. Facilitator API base URL.
FACILITATOR_URL=https://facilitator.daydreams.systems

# Starknet Configuration for Ekubo Market Watcher
# Optional. Starknet mainnet RPC URL for fetching pool events
STARKNET_RPC_URL=https://starknet-mainnet.g.alchemy.com/starknet/version/rpc/v0_9/acWedFT66z6F_9Zae2-OAKFe07QhhEIg
# Optional. Starknet testnet RPC URL for testing
STARKNET_TESTNET_RPC_URL=https://starknet-sepolia.g.alchemy.com/starknet/version/rpc/v0_9/acWedFT66z6F_9Zae2-OAKFe07QhhEIg
# Alternative RPC URLs (uncomment to use)
# STARKNET_RPC_URL=https://rpc.starknet.lava.builders
# STARKNET_RPC_URL=https://starknet-mainnet.public.blastapi.io

# Ekubo Configuration
# Optional. Ekubo Core contract addresses
EKUBO_CORE_MAINNET=0x00000005dd3D2F4429AF886cD1a3b08289DBcEa99A294197E9eB43b0e0325b4b
EKUBO_CORE_TESTNET=0x0444a09d96389aa7148f1aada508e30b71299ffe650d9c97fdaae38cb9a23384
# Optional. PoolInitialized event selector
POOL_INITIALIZED_EVENT_SELECTOR=0x25ccf80ee62b2ca9b97c76ccea317c7f450fd6efb6ed6ea56da21d7bb9da5f1

# Cache and Performance Settings
# Optional. Cache TTL in milliseconds
CACHE_TTL_MS=60000
# Optional. Maximum number of pools to cache
MAX_POOL_CACHE_SIZE=1000
# Optional. Approximate blocks per minute on Starknet
BLOCKS_PER_MINUTE=60
# Optional. Maximum lookback period in minutes
MAX_LOOKBACK_MINUTES=1440

# Logging
# Optional. Log level (debug, info, warn, error)
LOG_LEVEL=info

# Payments (optional)
# Optional. Address that receives x402 payments.
PAY_TO=0xb308ed39d67D0d4BAe5BC2FAEF60c66BBb6AE429
# Optional. Default price (in base units) when no entrypoint price is set.
DEFAULT_PRICE=1000

# Secrets
# Required. Used to sign payment requests with the agent wallet.
PRIVATE_KEY=

# Required for AXLLM - but can be ignored
OPENAI_API_KEY=THIS-WILL-NOT-BE-USED
